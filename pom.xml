			<artifactId>frms-base</artifactId>
			<version>4.1.1</version>
			<classifier>public</classifier>
		</dependency>
		<dependency>
			<groupId>cn.com.bsfit</groupId>
			<artifactId>bsfit-api</artifactId>
			<version>1.0.1</version>
		</dependency>
        <dependency>
            <groupId>com.fk</groupId>
            <artifactId>fk_dubbo_api</artifactId>
            <version>2.2.7-SNAPSHOT</version>
        </dependency>
